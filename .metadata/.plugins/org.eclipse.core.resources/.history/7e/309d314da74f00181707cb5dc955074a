import java.util.*;
import java.io.*;

class State6{
	public int noBusyServers = 0, noMeasurements = 0, N = 100, lambda = 4, sTime = 10, T = 4;
	
	Random slump = new Random();
	SimpleFileWriter W = new SimpleFileWriter("number.m", false);
	
	public void TreatEvent(Event x){
		switch (x.eventType){
		case G6.ARRIVAL:{
			if (noBusyServers < N){
				noBusyServers++;
				EventList.InsertEvent(G6.DEPARTURE, G6.time + sTime);
			}
			EventList.InsertEvent(G6.ARRIVAL, G6.time - (1.0/lambda)*Math.log(slump.nextDouble())); 
		} break;
		case G6.DEPARTURE:{
			noBusyServers--;			
		} break;
		case G6.MEASURE:{
			noMeasurements++;
			
			EventList.InsertEvent(G6.MEASURE, G6.time - (1.0 * T)*Math.log(slump.nextDouble()));
			W.println(String.valueOf(noBusyServers));
		} break;
		}
	}
}